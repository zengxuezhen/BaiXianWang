<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 对mybatis整体的配置 -->
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<!-- 给实体类起别名 -->
	<typeAliases>
		<typeAlias type="com.zl.pojo.SelfEmployedIdentify" alias="self"/>
		<typeAlias type="com.zl.pojo.CompanyInfo" alias="company"/>
		<typeAlias type="com.zl.pojo.AuthorizedOperator" alias="author"/>
		
	</typeAliases>
	<!-- 引用mapper映射文件 -->
	<mappers>
		<mapper resource="com/zl/dao/UpgradeSelfPurchaseDaoMapper.xml"/>
		<mapper resource="com/zl/dao/UpgradeCompanyPurchaseDaoMapper.xml"/>
	</mappers>
</configuration>